<IfModule mod_rewrite.c>
RewriteEngine on
RewriteCond %{HTTP_HOST} ^[\w|-|\.]+$
RewriteCond %{REQUEST_URI} !^/wp-core/
RewriteCond %{REQUEST_FILENAME} !-f
RewriteCond %{REQUEST_FILENAME} !-d
RewriteRule ^(.*)$ /wp-core/$1
RewriteCond %{HTTP_HOST} ^[\w|-|\.]+$
RewriteRule ^(/)?$ wp-core/index.php [L] 
</IfModule>